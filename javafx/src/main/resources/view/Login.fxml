<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.laohe.controller.LoginController">
   <children>
      <Label alignment="CENTER" contentDisplay="TOP" layoutX="106.0" layoutY="132.0" prefHeight="50.0" prefWidth="80.0" text="用户名:">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="106.0" layoutY="200.0" prefHeight="50.0" prefWidth="80.0" text="密码:">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="usernameText" layoutX="220.0" layoutY="146.0" promptText="请输入用户名" />
      <TextField fx:id="passwordText" layoutX="220.0" layoutY="214.0" promptText="请输入密码" />
      <Button fx:id="loginBtn" layoutX="157.0" layoutY="302.0" mnemonicParsing="false" onAction="#login" prefWidth="60.0" text="登陆" />
      <Button layoutX="239.0" layoutY="302.0" mnemonicParsing="false" prefWidth="60.0" text="注册" />
      <Button layoutX="317.0" layoutY="302.0" mnemonicParsing="false" text="忘记密码？" />
   </children>
</AnchorPane>
